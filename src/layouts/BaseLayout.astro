---
import BaseHead from "@/components/BaseHead.astro";
import Navigation from "@/components/global/Navigation.astro";
import Footer from "@/components/global/Footer.astro";
---

<html lang="en" class="selection:bg-teal-400 selection:text-black">
  <head>
    <BaseHead />
  </head>
  <body class="bg-black h-screen w-screen">
    <div class="w-screen fisheye">
    <Navigation />
    <slot />
    <Footer />
  </div>
    <svg>
      <defs>
        <filter id="fisheye">
          <feImage
            xlink:href="data:image/png;base64,"
            result="lightMap"
            x="0"
            y="0"
            width="1000"
            height="1000"
            ></feImage>
          <feDisplacementMap
            in2="lightMap"
            in="SourceGraphic"
            xChannelSelector="R"
            yChannelSelector="G"
            scale="70"></feDisplacementMap>
          <feComposite operator="in" in2="lightMap"></feComposite>
        </filter>

        <filter id="inner-glow">
          <feFlood flood-color="transparent"></feFlood>
          <feComposite in2="SourceAlpha" operator="out"></feComposite>
          <feGaussianBlur stdDeviation="100" result="blur"></feGaussianBlur>
          <feComposite operator="atop" in2="SourceGraphic"></feComposite>
        </filter>
        <filter id="bloom">
          <feGaussianBlur in="SourceGraphic" stdDeviation="10"></feGaussianBlur>
          <feComposite in2="SourceGraphic" operator="arithmetic" k2="0.9" k3="1.4"
          ></feComposite>
        </filter>
      </defs>
    </svg>
    <script>
      fun
    </script>
  </body>
</html>
